# .env.example - Copy this file to .env and fill in your API keys.

GEMINI_API_KEY="your-gemini-api-key"

# Google Programmable Search Engine (CSE) credentials for web_search tool
# API key for Google CSE JSON API
GOOGLE_SEARCH_API_KEY="your-google-cse-api-key"
# Search Engine ID (aka `cx`) configured to search the entire web
GOOGLE_SEARCH_ENGINE_ID="your-cse-engine-id"

# Ethereum / mcp-wallet defaults (Anvil)
# Local Anvil RPC URL
ETH_RPC_URL="http://127.0.0.1:8545"
# Local Anvil chain id
CHAIN_ID="31337"
# Path to wallet file managed by mcp-wallet (PoC)
WALLET_FILE="./.wallet.json"
# Optional gas params
GAS_LIMIT="2100000"
GAS_PRICE="1000000000"
